<Window x:Class="ZoDream.Spider.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZoDream.Spider"
        xmlns:sctl="clr-namespace:ZoDream.Shared.Controls;assembly=ZoDream.Shared.Controls"
        mc:Ignorable="d"
        Title="ZoDream Spider" Height="450" Width="800"
        WindowStartupLocation="CenterScreen"
        Unloaded="Window_Unloaded">
    <Grid Background="{DynamicResource WindowBodyBrush}">
        <Frame x:Name="InnerFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
